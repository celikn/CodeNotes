 // Arrow functions 
    let x=()=>console.log("Hello");  // single line
    x();

    // Arrow functions 
    let y=()=>{
        return "Hello";  // multiple line
    };

    console.log(y());

    let add=(num1,num2)=>{
        return num1+num2
    }

    let div=(num1,num2)=>{
        return num1/num2
    }

    console.log(add(5,4));
    console.log(div(20,4));
